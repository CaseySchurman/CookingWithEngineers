<% provide(:title, @page_title ) %>
<!-- Displays all users -->
<%= will_paginate %>
<div class = "content">
    <h2>All Users</h2>
<table class = "centerStyle">
    <%= render 'users/table_header' %>
    <% @users.each do |user| %>
        <tr>
            <td><%= link_to user.username, user %></td>
            <td><%= user.email %></td>
        </tr>
    <% end %>
</table>

<%= will_paginate %>
</div>